#include <cstdio>
#include <algorithm>
#include <cstring>
using namespace std;
char s[16][4];
int a[16];
int main() {
    for (int i = 0; i < 16; ++ i) {
	scanf("%s", s[i]);
    }
    for (int i = 0; i < 16; ++ i) {
	a[i] = atoi(s[i]);
    }
    for (int i = 0; i < 16; ++ i) {
	printf("%d\n", &a[i]);
    }
    return 0;
}
