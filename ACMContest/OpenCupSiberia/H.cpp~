#include <cstdio>
#include <algorithm>
#include <cstring>
using namespace std;
const int N = 10010;
struct node {
    int a, b;
}p[N];
int main() {
    int n, m;
    scanf("%d%d", &n, &m);
    for (int i = 0; i < n; ++ i) {
	scanf("%d", &a[i]);
    }
    for (int i = 0; i < m; ++ i) {
	scanf("%d%d", &p[i].a, &p[i].b);
    }
    return 0;
}
